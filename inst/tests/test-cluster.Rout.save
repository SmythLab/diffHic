> crisscross <- function(id1, id2) {
+ 	out <- split(id1, id2)
+ 	if (!all(sapply(out, FUN=function(x) { length(unique(x))==1L }))) {
+ 		return(FALSE)
+ 	}
+ 	out <- split(id2, id1)
+ 	if (!all(sapply(out, FUN=function(x) { length(unique(x))==1L }))) {
+ 		return(FALSE)
+ 	}
+ 	return(TRUE)
+ }
